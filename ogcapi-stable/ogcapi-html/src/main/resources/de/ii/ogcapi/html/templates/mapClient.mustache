{{#mapClient}}
<script type="text/javascript">
  globalThis._map = {
      container: 'map',
      interactive: {{interactive}},
    {{#savePosition}}
      savePosition: {{.}},
    {{/savePosition}}
    {{#styleUrl}}
      styleUrl: '{{{.}}}',
      {{#removeZoomLevelConstraints}}
        removeZoomLevelConstraints: {{.}},
      {{/removeZoomLevelConstraints}}
      {{#useBounds}}
        {{#bounds}}
          bounds: [[{{minLng}},{{minLat}}],[{{maxLng}},{{maxLat}}]],
          drawBounds: {{drawBounds}},
        {{/bounds}}
      {{/useBounds}}
    {{/styleUrl}}
    {{^styleUrl}}
      {{#backgroundUrl}}
        backgroundUrl: '{{{.}}}',
      {{/backgroundUrl}}
      {{#attribution}}
        attribution: '{{{.}}}',
      {{/attribution}}
      {{#zoom}}
        zoom: {{.}},
        center: {{center}},
      {{/zoom}}
      {{^zoom}}
        {{#bounds}}
          bounds: [[{{minLng}},{{minLat}}],[{{maxLng}},{{maxLat}}]],
          drawBounds: {{drawBounds}},
        {{/bounds}}
      {{/zoom}}
    {{/styleUrl}}
    {{#data}}
     {{#isData}}
      dataUrl:  {{{url}}},
     {{/isData}}
     {{^isData}}
      dataUrl:  '{{{url}}}',
     {{/isData}}
      dataType: '{{type}}',
      dataLayers: {
        {{#layers.entries}}
        '{{key}}': [{{#value}}'{{.}}',{{/value}}],
        {{/layers.entries}}
      },
    {{/data}}
    {{#defaultStyle}}
      defaultStyle: {
          color: '{{color}}',
          opacity: {{opacity}},
          circleRadius: {{circleRadius}},
          circleMinZoom: {{#circleMinZoom}}{{.}}{{/circleMinZoom}}{{^circleMinZoom}}undefined{{/circleMinZoom}},
          circleMaxZoom: {{#circleMaxZoom}}{{.}}{{/circleMaxZoom}}{{^circleMaxZoom}}undefined{{/circleMaxZoom}},
          lineWidth: {{lineWidth}},
          lineMinZoom: {{#lineMinZoom}}{{.}}{{/lineMinZoom}}{{^lineMinZoom}}undefined{{/lineMinZoom}},
          lineMaxZoom: {{#lineMaxZoom}}{{.}}{{/lineMaxZoom}}{{^lineMaxZoom}}undefined{{/lineMaxZoom}},
          fillOpacity: {{fillOpacity}},
          outlineWidth: {{outlineWidth}},
          polygonMinZoom: {{#polygonMinZoom}}{{.}}{{/polygonMinZoom}}{{^polygonMinZoom}}undefined{{/polygonMinZoom}},
          polygonMaxZoom: {{#polygonMaxZoom}}{{.}}{{/polygonMaxZoom}}{{^polygonMaxZoom}}undefined{{/polygonMaxZoom}},
      },
    {{/defaultStyle}}
    {{#popup}}
      popup: '{{.}}',
    {{/popup}}
      layerGroupControl: [
    {{#layerGroupControl}}
        {
        id : '{{key}}',
        type: 'source-layer',
        'sub-layers': [
        {{#value}}
          {id: '{{.}}', layers: ['{{.}}']},
        {{/value}}
        ],
        },
    {{/layerGroupControl}}
      ],
      tileMatrixSets: [
      {{#tileCollections}}
        {
        {{#.}}
          {{key}}: '{{value}}',
        {{/.}}
        },
      {{/tileCollections}}
      ],
      setCurrentTileMatrixSet: () => {},
  }
</script>

  {{#isMapLibre}}
    {{> app-maplibre}}
  {{/isMapLibre}}
  {{#isOpenLayers}}
    {{> app-openlayers}}
  {{/isOpenLayers}}
  {{#isCesium}}
    {{> data-cesium}}
    {{> app-cesium}}
  {{/isCesium}}

{{/mapClient}}

