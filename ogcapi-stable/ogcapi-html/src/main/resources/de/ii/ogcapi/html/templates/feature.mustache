<div>
    {{#inCollection}}
      {{#fromStoredQuery}}
        <h4 class="mt-3 mb-1"><span>{{name}}</span></h4>
      {{/fromStoredQuery}}
      {{^fromStoredQuery}}
        <h4 class="mt-3 mb-1"><a href="{{#currentUrlWithSegment}}{{idValue}}{{/currentUrlWithSegment}}{{additionalParams}}"><span>{{name}}</span></a></h4>
      {{/fromStoredQuery}}
      <span class="d-none">{{#currentUrlWithSegmentClearParams}}{{idValue}}{{/currentUrlWithSegmentClearParams}}</span>
    {{/inCollection}}
    {{^inCollection}}
        <h1>{{name}}</h1>
    {{/inCollection}}
    {{#properties}}
        {{> property}}
    {{/properties}}
    {{#links}}
        <dl class="dl-horizontal dl-horizontal-no-space">
            <dt>{{name}}</dt>
        <dd>
            <ul class="list-unstyled">
            {{#children}}
                <li>
                    <a href="{{value.value}}" target="_blank">{{value.name}}</a>
                </li>
            {{/children}}
                </ul>
        </dd>
        </dl>
    {{/links}}
</div>
