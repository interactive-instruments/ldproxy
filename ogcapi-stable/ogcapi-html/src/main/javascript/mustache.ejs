<% if (htmlWebpackPlugin.files.favicon) { %><link rel="shortcut icon" type="image/x-icon" href="<%=htmlWebpackPlugin.files.favicon%>"><% } %>

<%= htmlWebpackPlugin.tags.headTags.filter((tag) => 
(tag.tagName === 'link' && tag.attributes.rel && tag.attributes.rel === 'stylesheet') ||
(tag.tagName === 'script' && tag.attributes.src &&
tag.attributes.src.indexOf(`assets/${htmlWebpackPlugin.options.name}.`) > -1 &&
tag.attributes.src.indexOf('assets/style-') === -1) ).join('\n') %>
