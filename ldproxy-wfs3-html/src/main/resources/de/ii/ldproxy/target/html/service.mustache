{{#wfs3Dataset}}

    {{> header}}

    <div itemscope itemtype="http://schema.org/Dataset">
        <h1 itemprop="name">{{title}}</h1>
        <span itemprop="description">{{{description}}}</span>
        <p itemprop="url" class="d-none">{{canonicalUrl}}</p>
        {{#catalogUrl}}
            <div itemprop="includedInDataCatalog" itemscope itemtype="http://schema.org/Datacatalog" class="d-none">
                <div itemprop="url">{{.}}</div>
            </div>
        {{/catalogUrl}}
        {{#metadata}}
            <ul itemprop="keywords" class="d-none">
                {{#keywords}}
                    <li>
                        {{.}}
                    </li>
                {{/keywords}}
            </ul>
        {{/metadata}}
        {{#distributions}}
            <ul class="d-none">
                {{#keywords}}
                    <li itemprop="distribution" itemscope itemtype="http://schema.org/DataDownload">
                        <span itemprop="name">{{name}}</span><span itemprop="encodingFormat">{{encodingFormat}}</span><span itemprop="url">{{url}}</span>
                    </li>
                {{/keywords}}
            </ul>
        {{/distributions}}
        <br/>
        <br/>
        <br/>
        <div class="row my-3">
            <div class="col-md-2 font-weight-bold">Collections</div>
            <div class="col-md-10">
                <ul class="list-unstyled">
                    {{#featureTypes}}
                        <li>
                            <a href="{{url}}">{{title}}</a>
                        </li>
                    {{/featureTypes}}
                    {{^featureTypes}}
                        <li>None</li>
                    {{/featureTypes}}
                </ul>
            </div>
        </div>
        <div class="row my-3">
            <div class="col-md-2 font-weight-bold">API Definition</div>
            <div class="col-md-10">
                <a href="{{apiUrl}}">OpenAPI 3.0</a>
            </div>
        </div>
    </div>

    <div class="row my-3">
            <div class="col-md-2 font-weight-bold">Maps</div>
            <div class="col-md-10">
                <ul class="list-unstyled">


                    {{#styles}}
                         <li>
                            <a itemprop="url" href="{{styleUrl}}" target="_blank">{{styleId}}</a>
                        </li>
                    {{/styles}}
                    {{^styles}}
                        <li>None</li>
                    {{/styles}}
                </ul>
            </div>
    </div>

        {{#dataSourceUrl}}
            <div class="row my-3">
                <div class="col-md-2 font-weight-bold">Data source</div>
                <div class="col-md-10">
                    <a itemprop="isBasedOn" href="{{.}}" target="_blank">{{.}}</a>
                </div>
            </div>
        {{/dataSourceUrl}}

        {{#metadata}}
            <div class="row my-3">
                <div class="col-md-2 font-weight-bold">Provider</div>
                <div class="col-md-10">
                    <ul class="list-unstyled" itemprop="creator" itemscope itemtype="http://schema.org/Organization">
                        {{#contactName}}
                            <li itemprop="name">{{.}}</li>
                        {{/contactName}}
                        {{#contactUrl}}
                            <li itemprop="url"><a href="{{.}}" target="_blank">{{.}}</a></li>
                        {{/contactUrl}}
                        <li itemprop="contactPoint" itemscope itemtype="http://schema.org/ContactPoint">
                            <span class="d-none" itemprop="contactType">technical support</span>
                            <ul class="list-unstyled">
                                {{#contactEmail}}
                                    <li itemprop="email">{{.}}</li>
                                {{/contactEmail}}
                                {{#contactPhone}}
                                    <li itemprop="telephone">{{.}}</li>
                                {{/contactPhone}}
                                {{#contactUrl}}
                                    <li itemprop="url" class="d-none">{{.}}</li>
                                {{/contactUrl}}
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
            {{#licenseName}}
                <div class="row my-3">
                    <div class="col-md-2 font-weight-bold">License</div>
                    <div class="col-md-10">
                        <div itemprop="license">{{.}}</div>
                    </div>
                </div>
            {{/licenseName}}
        {{/metadata}}
        {{#temporalCoverage}}
                    <div itemprop="temporalCoverage" class="d-none">{{.}}</div>
        {{/temporalCoverage}}
        {{#spatialCoverage}}
            <div itemprop="spatialCoverage" itemscope itemtype="http://schema.org/Place" class="d-none"><div itemprop="geo" itemscope itemtype="http://schema.org/GeoShape"><div itemprop="box">{{.}}</div></div></div>
        {{/spatialCoverage}}


    </div>

    {{> footer}}

{{/wfs3Dataset}}
