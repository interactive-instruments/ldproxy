<body>

<!-- Fixed navbar -->
<nav class="navbar navbar-default navbar-static-top">
    <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar"
                aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="https://github.com/interactive-instruments/ldproxy" target="_blank">ldproxy</a>
    </div>
    <div class="container">
        <div id="navbar" class="navbar-collapse collapse">
            <ol class="breadcrumb navbar-text">
                {{#breadCrumbs}}
                    {{#url}}
                        <li><a href="{{path}}{{.}}">{{label}}</a></li>
                    {{/url}}
                    {{^url}}
                        <li class="active">{{label}}</li>
                    {{/url}}
                {{/breadCrumbs}}
            </ol>
            <div class="navbar-right nav-formats">
                {{#formats}}
                    <span class="navbar-text"><a href="{{path}}{{query}}{{url}}" target="_blank">{{label}}</a></span>
                {{/formats}}
            </div>
        </div>
    </div>
</nav>

